#Variables
x="Presidency University"
print(x, '-', type(x))

x=20
print(x, '-', type(x))

x=20.5
print(x, '-', type(x))

x= True
print(x, '-', type(x))

x=1j
print(x, '-', type(x))

x=['apple', 'banana', 'cherry']
print(x, '-', type(x))

x=frozenset({'apple', 'banana', 'cherry'})
print(x, '-', type(x))
